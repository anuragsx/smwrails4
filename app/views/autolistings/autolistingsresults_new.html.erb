<div id="content">
  <div class="page">
    <div class="rightcontent">
      <div class="contact"> <span>Find Your</span><span class="whip"> Whip</span>
        <div class="checkbox">
          <form action="#">
            <div class="question">
              <label class="question">01. What?</label>
              <div class="clear"></div>
              <div class="check">
                <input type="radio" name="autos"/>
                <label>Autos</label>
              </div>
              <div class="check">
                <input type="radio" name="autos"/>
                <label>Marine</label>
              </div>
              <div class="check">
                <input type="radio" name="autos"/>
                <label>Motorcycles</label>
              </div>
              <div class="check">
                <input type="radio" name="autos"/>
                <label>Power Sports</label>
              </div>
              <div class="clear"></div>
            </div>
            <div class="question">
              <label class="question">02. Category</label>
              <div class="select selectParent">
                <select>
                  <option>Select a category</option>
                  <option>category1</option>
                  <option>category2</option>
                  <option>category3</option>
                </select>
              </div>
              <div class="clear"></div>
            </div>
            <div class="question">
              <label class="question">03. Make and Model</label>
              <div class="select">
                <select class="leftselect">
                  <option>Select a Make</option>
                  <option>category1</option>
                  <option>category2</option>
                  <option>category3</option>
                </select>
                <select class="rightselect">
                  <option>Select a Model</option>
                  <option>category1</option>
                  <option>category2</option>
                  <option>category3</option>
                </select>
              </div>
              <div class="clear"></div>
            </div>
            <div class="question">
              <label class="question">04. State</label>
              <div class="select">
                <select>
                  <option>Select a State</option>
                  <option>category1</option>
                  <option>category2</option>
                  <option>category3</option>
                </select>
              </div>
              <div class="clear"></div>
            </div>
            <a href="#" class="advance">Advance Search</a>
            <input type="submit" class="submit" value="Search" />
          </form>
        </div>
      </div>
      <div class="ad1"> <img src="/assets/ad1.jpg" width="300" height="125" /> </div>
      <div class="ad2"> <img src="/assets/ad2.jpg" width="300" height="125" /> </div>
      <div class="ad3"> <img src="/assets/ad3.jpg" width="300" height="250" /> </div>
    </div>
    <div class="leftcontent">
      <div class="lsitarea"> <img class="p_img" src="/assets/listingpageimage.png" width="725" height="276" />
        <div class="pagination">
          <div class="paging">
            <span class="font">Page 01 of 06</span> <img width="171" height="24" src="/assets/pagination.png">
          </div>


          <div class="short_by">
            <span class="sort font">Sort by:</span>
            <form class="float_left" action="#">
              <select>
                <option>Date Added</option>
                <option>No Availability</option>
              </select>
            </form>
          </div>
          <div class="list_map">
            <span class="sort">List</span><img src="/assets/list.png" class="list" width="42" height="17" /> <span class="sort">Map</span>
          </div>

          <div class="clear"></div>
        </div>

        <% if current_user %>
            <% if @perfectresults_count > 0 %>
                <% @perfectresults.each do |listing| %>
                    <div class="post">
                      <div class="leftpost"> <img class="pos_img" src="/assets/innersmallsliderimage2.png" width="114" height="91" />
                        <div class="middilepost">
                          <div class="inline">
                            <h2><%=h listing.model %></h2>

                            <span class="mileage">Milage : <%=h listing.mileage %></span> </div>
                          <%unless listing.owner.nil?%>
                          <% if listing.owner.city && listing.owner.state %>
                              <span><%=h listing.owner.city %>, <%=h listing.owner.state %></span> <img src="/assets/bubble.png" width="13" height="15" />
                          <%end%>
                          <%end%>
                          <p class="description"></p>
                          <a href="#">View Details &raquo;</a> </div>
                      </div>
                      <div class="swap"> <span><sup>$</sup><%=h listing.price %></span> </div>
                      <div class="clear"></div>
                    </div>
                <%end%>
            <%end%>
        <%end%>

        <% unless  @listings.blank?%>
            <% @listings.each do |listing| %>
                <div class="post">
                  <div class="leftpost"> <img class="pos_img" src="/assets/innersmallsliderimage2.png" width="114" height="91" />
                    <div class="middilepost">
                      <div class="inline">
                        <h2><%=h listing.model %></h2>
                        <span class="mileage">Milage : <%=h listing.mileage %></span> </div>
                      <%unless listing.owner.nil?%>
                          <% if listing.owner.city && listing.owner.state %>
                              <span><%=h listing.owner.city %>, <%=h listing.owner.state %></span> <img src="/assets/bubble.png" width="13" height="15" />
                          <%end%>
                      <%end%>

                      <p class="description"></p>
                      <%= link_to "View Details", listing_path(listing)%></div>
                  </div>
                  <div class="swap"> <span><sup>$</sup><%=h listing.price %></span> </div>
                  <div class="clear"></div>
                </div>
            <%end%>
        <%end%>

        <div class="paginationextra"> <span>Page 01 of 06</span> <img src="images/pagination.png" width="171" height="24" /> </div>
      </div>
      <div class="clear"></div>
    </div>
  </div>
  <div class="clear"></div>
</div>