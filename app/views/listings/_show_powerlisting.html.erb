      <li><b>Year</b>: <%= @listing.year %></li>
      <li><b>Category</b>: <%=h @listing.category %></li>
      <li></li>
    </ul>
    <ul>
      <li><b>Sub Category</b>: <%=h @listing.subcategory %></li>
      <li><b>Make</b>: <%=h @listing.make %></li>
      <li></li>
    </ul>
    <ul>
      <li><b>Price</b>: <%=h number_to_currency(@listing.price, :precision => 0) %></li>
