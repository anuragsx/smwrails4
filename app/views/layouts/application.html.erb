<%
if (controller.controller_name == 'users' && controller.action_name == 'new')
  @class = "whipdetails twocolumns register"
elsif (controller.controller_name == 'home' && controller.action_name == 'about')
  @class = "fullwidth"
elsif (controller.controller_name == 'home' && controller.action_name == 'faq')
  @class = "fullwidth"
elsif (controller.controller_name == 'home' && controller.action_name == 'contact')
  @class = "fullwidth"
elsif (controller.controller_name == 'home' && controller.action_name == 'privacypolicy')
  @class = "fullwidth"
elsif (controller.controller_name == 'home' && controller.action_name == 'visitoragreement')
  @class = "fullwidth"
elsif (controller.controller_name == 'autolistings' && controller.action_name == 'autolistingsresults_new')
  @class = "whipdetails"
elsif (controller.controller_name == 'messages' && controller.action_name == 'inbox')
  @class = "fullwidth emailpage"
elsif (controller.controller_name == 'messages' && controller.action_name == 'sent')
  @class = "fullwidth emailpage"
elsif (controller.controller_name == 'messages' && controller.action_name == 'show')
  @class = "fullwidth emailpage"
elsif (controller.controller_name == 'messages' && controller.action_name == 'reply')
  @class = "fullwidth emailpage"
else
    @class = "whipdetails twocolumns register"
end
%>

